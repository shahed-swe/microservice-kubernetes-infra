# Local Development Environment Variables
# Copy to .env and customize as needed

# PostgreSQL Global Database
DB_GLOBAL_PASSWORD=ChangeMe123!Global
DB_GLOBAL_HOST=localhost
DB_GLOBAL_PORT=5432
DB_GLOBAL_NAME=cdn_global
DB_GLOBAL_USER=cdn_admin_global

# PostgreSQL Regional Database
DB_REGIONAL_PASSWORD=ChangeMe123!Regional
DB_REGIONAL_HOST=localhost
DB_REGIONAL_PORT=5433
DB_REGIONAL_NAME=cdn_regional_uswest
DB_REGIONAL_USER=cdn_admin_regional

# MinIO Configuration
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin123
MINIO_ENDPOINT=localhost:9000
MINIO_CONSOLE_URL=http://localhost:9001
MINIO_BUCKET=cdn-config-bundles
MINIO_USE_SSL=false

# pgAdmin Configuration
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=admin123

# Application Configuration
ENVIRONMENT=development
LOG_LEVEL=debug
ENABLE_MONITORING=true
